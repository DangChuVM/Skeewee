<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<include layout="@layout/toolbar_improved" />
<LinearLayout
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<LinearLayout
		android:orientation="vertical"
		android:id="@id/holder"
		android:padding="8.0dip"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<LinearLayout
			android:gravity="center_vertical"
			android:orientation="horizontal"
			android:id="@id/first_lin"
			android:paddingLeft="8.0dip"
			android:paddingTop="4.0dip"
			android:paddingBottom="4.0dip"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			<TextView
				android:textSize="14.0sp"
				android:textStyle="bold"
				android:textColor="#ff212121"
				android:id="@id/jar_instruction"
				android:paddingTop="8.0dip"
				android:paddingBottom="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="jar:" />
			<EditText android:textSize="14.0sp"
				android:textColor="#ff000000"
				android:textColorHint="#ff607d8b"
				android:id="@id/jar_edittext"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:hint="jar path here"
				android:singleLine="true"
				android:layout_weight="1.0" />
			<Button
				android:textSize="12.0sp"
				android:textColor="#ff000000"
				android:id="@id/btn_select_jar"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="4.0dip"
				android:text="select" />
		</LinearLayout>
		<LinearLayout
			android:orientation="horizontal"
			android:id="@id/second_lin"
			android:paddingLeft="8.0dip"
			android:paddingTop="4.0dip"
			android:paddingBottom="4.0dip"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			<TextView
				android:textSize="14.0sp"
				android:textStyle="bold"
				android:textColor="#ff212121"
				android:id="@id/output_instruction"
				android:paddingTop="8.0dip"
				android:paddingBottom="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="output:" />
			<EditText
				android:textSize="14.0sp"
				android:textColor="#ff000000"
				android:textColorHint="#ff607d8b"
				android:id="@id/output_edittext"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:hint="output path here"
				android:singleLine="true"
				android:layout_weight="1.0" />
			<Button
				android:textSize="12.0sp"
				android:textColor="#ff000000"
				android:id="@id/btn_select_output"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="4.0dip"
				android:text="select" />
		</LinearLayout>
		<LinearLayout
			android:orientation="horizontal"
			android:id="@id/third_lin"
			android:paddingLeft="8.0dip"
			android:paddingTop="4.0dip"
			android:paddingBottom="4.0dip"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			<TextView
				android:textSize="14.0sp"
				android:textStyle="bold"
				android:textColor="#ff212121"
				android:id="@id/textview3"
				android:paddingTop="8.0dip"
				android:paddingBottom="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="android.jar:" />
			<EditText
				android:textSize="14.0sp"
				android:textColor="#ff000000"
				android:textColorHint="#ff607d8b"
				android:id="@id/anjar_edittext"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:hint="android.jar path (optional)"
				android:singleLine="true"
				android:layout_weight="1.0" />
			<Button
				android:textSize="12.0sp"
				android:textColor="#ff000000"
				android:id="@id/btn_select_anjar"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="4.0dip"
				android:text="select" />
		</LinearLayout>
		<LinearLayout
			android:orientation="horizontal"
			android:id="@id/linear3"
			android:paddingLeft="4.0dip"
			android:paddingTop="8.0dip"
			android:paddingRight="6.0dip"
			android:paddingBottom="8.0dip"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			<CheckBox
				android:textSize="12.0sp"
				android:textColor="#ff000000"
				android:id="@id/c_release"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="--release" />
			<CheckBox
				android:textSize="12.0sp"
				android:textColor="#ff000000"
				android:id="@id/c_int"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:checked="true"
				android:text="--intermediate" />
			<CheckBox
				android:textSize="12.0sp"
				android:textColor="#ff000000"
				android:id="@id/c_fileperclass"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="--file-per-class" />
		</LinearLayout>
		<EditText
			android:textSize="12.0sp"
			android:textColor="#ff000000"
			android:textColorHint="#ff607d8b"
			android:id="@id/more_args_edittext"
			android:padding="8.0dip"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:hint="extra args" />
		<Button
			android:textSize="12.0sp"
			android:textColor="#ffffffff"
			android:id="@id/run_btn"
			android:padding="8.0dip"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4.0dip"
			android:layout_marginRight="4.0dip"
			android:text="RUN D8" />
		<LinearLayout
			android:orientation="horizontal"
			android:id="@id/logger_holder"
			android:layout_width="fill_parent" android:layout_height="wrap_content">
			<TextView
				android:textSize="12.0sp"
				android:textColor="#ff000000"
				android:id="@id/log_txt"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="You can put either an existing directory or a .zip file as the output path."
				android:layout_weight="1.0" />
			<ProgressBar
				android:id="@id/d8_progress"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:indeterminate="true"
				style="?android:progressBarStyle" />
		</LinearLayout>
		<LinearLayout
			android:orientation="horizontal"
			android:id="@id/divider_logger_holder"
			android:background="#ff9e9e9e"
			android:padding="8.0dip"
			android:layout_width="fill_parent"
			android:layout_height="1.0dip"
			android:layout_marginLeft="4.0dip"
			android:layout_marginRight="4.0dip" />
		<LinearLayout
			android:gravity="center_vertical"
			android:orientation="horizontal"
			android:id="@id/logger_txt_holder"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content">
			<TextView
				android:textSize="12.0sp"
				android:textStyle="bold"
				android:textColor="#ff2962ff"
				android:gravity="center_vertical"
				android:id="@id/textview4"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Logger"
				android:layout_weight="1.0" />
			<TextView
				android:textSize="12.0sp"
				android:textStyle="bold"
				android:textColor="#ff2962ff"
				android:id="@id/clear_log"
				android:padding="8.0dip"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Clear" />
		</LinearLayout>
		<ScrollView
			android:id="@id/scroll_view"
			android:padding="8.0dip"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			<TextView
				android:textSize="12.0sp"
				android:textColor="#ff000000"
				android:id="@id/logger_preview_txt"
				android:padding="8.0dip"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content" />
		</ScrollView>
	</LinearLayout>
</LinearLayout>
</LinearLayout>